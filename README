DEPENDS:

	+ gtkmm-2.4			- http://www.gtkmm.org/
	+ cURL (>= 7.18)	- http://curl.haxx.se/
	+ pcre++ (>= 0.9)	- http://www.daemon.de/PCRE
	+ zlibc (>= 0.9)	- http://zlibc.linux.lu/

HOW-TO-COMPILE:	# make

HOW-TO-INSTALL:	# make install

HOW-TO-REMOVE:	# make unistall

